<%= current_user.email %>
<%= link_to 'Edit Profile' , edit_user_registration_path, class:"btn btn-warning" %>
<%= link_to 'LogOut', destroy_user_session_path, method: :delete, class:"btn btn-success" %>
<table>
  <tr>
    <th>Email</th>
    <th colspan="3"></th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
  	<td><%= link_to 'Edit', edit_admin_user_path(user) %></td>
  	<td><%= link_to 'Show', admin_user_path(user) %></td>
  	<td><%= link_to 'Destroy', [:admin, user], method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
  <% end -%>
</table>