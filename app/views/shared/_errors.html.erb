<% if object.errors.any? %>
  <% object.errors.each do |error| %>
    <div class='alert alert-danger alert-dismissible fade show' role='alert'>
        <%= error.full_message %>
        <button type='button' class='btn-close abc' data-bs-dismiss='alert' aria-label='Close'></button>
    </div>
  <% end %>
<% end %>
